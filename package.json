{
  "name": "mixtube",
  "description": "Mixtube umbrella module",
  "version": "1.0.0-alpha.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jraoult/mixtube.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "scripts": {
    "preinstall:app": "cd app && npm install",
    "preinstall:build": "cd build && npm install",
    "preinstall": "npm-run-all --parallel preinstall:*",
    "start": "npm run debug",
    "debug": "npm run gulp-build -- --watch --serve",
    "deploy:gh": "npm run gulp-build -- deploy:gh --baseUrl /mixtube/ --production",
    "gulp-build": "build/node_modules/.bin/gulp --cwd build build"
  },
  "dependencies": {
    "npm-run-all": "^3.1.0"
  }
}
